{
    "manifest_version": 3,
    "name": "Hacker News Lookup",
    "version": "1.0",
    "description": "Find Hacker News posts for the current page",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
    },
    "background": {
      "service_worker": "background.js"
    }
  }